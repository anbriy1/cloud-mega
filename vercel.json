{
  "builds": [
    {
      "src": "vercel.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/vercel.js"
    },
    {
      "source": "/upload",
      "destination": "/vercel.js"
    },
    {
      "source": "/",
      "destination": "/login.html"
    }
  ]
}
